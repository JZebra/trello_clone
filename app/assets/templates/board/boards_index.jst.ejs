<h2 class="col-sm-12">My Boards</h2>

<ul class="boards">
  <% collection.each( function (board) { %>
    <li>
      <a href="#/boards/<%= board.escape('id') %>"><%= board.escape("title") %></a>
    </li>
    <button id="delete_board" data-id='<%= board.id %>'>Delete board</button>
  <% }); %>
</ul>

<div class="board-new"></div>

